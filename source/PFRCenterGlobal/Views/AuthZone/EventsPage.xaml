<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:resources="clr-namespace:PFRCenterGlobal.Helpers"
             Title="{resources:Translate TitleEvents}"
             x:Class="PFRCenterGlobal.Views.AuthZone.EventsPage">

    <ContentPage.Content>
        <Grid x:Name="GridRoot" Style="{StaticResource GridRootStyle}" >
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <!--<controls:Title Grid.Row="0" Text=""></controls:Title>-->

            <Grid Grid.Row="1">
                <Label Text="События отсутствуют" HorizontalTextAlignment="Center" VerticalOptions="Center" VerticalTextAlignment="Center"/>
            </Grid>
        </Grid>
    </ContentPage.Content>
</ContentPage>
